<div class="sessions_new">
  <h1>ログイン</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>

  <%= f.label :Eメール %>
  <br>
  <%= f.email_field :email %>
<br>
  <%= f.label :パスワード %>
  <br>
  <%= f.password_field :password %>
<br>
<br>
  <%= f.submit "ログイン" %>
<% end %>
</div>
